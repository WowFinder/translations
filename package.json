{
    "name": "@wowfidner/translations",
    "author": "Edurne Pascual <herenvardo@gmail.com>",
    "version": "0.0.0",
    "description": "Localization assets for WowFinder apps and modules",
    "scripts": {
        "lint:base": "node not-implemented eslint . --ext js,ts",
        "lint:stricter": "node not-implemented eslint . --ext js,ts --config .eslintrc-stricter.js",
        "lint": "npm-run-all -sc lint:base lint:stricter",
        "lint:fix": "node not-implemented eslint . --ext js,ts --fix",
        "format": "prettier --write \"{src,translations}/**/*.{js,ts,json,json5}\"",
        "format:check": "prettier --check \"{src,translations}/**/*.{js,ts,json,json5}\"",
        "test": "node not-implemented jest",
        "test:coverage": "node not-implemented jest --coverage",
        "tsc": "node not-implemented tsc",
        "checks:quick": "npm-run-all -pc lint tsc",
        "checks:standard": "npm-run-all -pc lint tsc test",
        "checks:full": "npm-run-all -sc lint:fix format build test:coverage",
        "checks:pedantic": "npm-run-all -sc lint format:check build test:coverage",
        "build:pre": "node not-implemented rimraf dist",
        "build:tsc": "node not-implemented tsc",
        "build": "npm-run-all -s build:pre build:tsc",
        "prepublishOnly": "yarn run build",
        "prepare": "yarn run build"
    },
    "keywords": [],
    "license": "MIT",
    "dependencies": {
        "i18next": "^23.10.0",
        "i18next-browser-languagedetector": "^7.1.0",
        "json5": "^2.2.3",
        "path-browserify": "^1.0.1"
    },
    "packageManager": "yarn@4.1.1",
    "devDependencies": {
        "npm-run-all": "^4.1.5",
        "prettier": "^3.2.5"
    }
}
