{
    "compilerOptions": {
        "target": "es2021",
        "allowJs": true,
        "module": "commonjs",
        "lib": ["ES6"],
        "esModuleInterop": true,
        "noImplicitAny": true,
        "sourceMap": true,
        "baseUrl": "./src",
        "outDir": "dist",
        "strict": true,
        "moduleResolution": "node",
        "resolveJsonModule": true
    },
    "include": ["src/**/*"],
    "exclude": []
}
